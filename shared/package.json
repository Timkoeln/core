{
  "name": "@awdware/gah-shared",
  "version": "0.0.39",
  "description": "Provides types and interfaces for gah",
  "main": "./lib/index.js",
  "scripts": {
    "prebuild": "del-cli lib",
    "lint": "eslint src/** --fix",
    "build": "yarn run prebuild && tsc -p . && node ts-version-support.js",
    "drop": "yarn run build && yarn publish --access public --no-git-tag-version",
    "schema": "node generate-schema.js"
  },
  "author": "LoaderB0T",
  "license": "MIT",
  "private": false,
  "dependencies": {},
  "devDependencies": {
    "@awdware/eslint-config-nodejs": "^0.0.6",
    "@types/node": "^14.6.0",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "del-cli": "^3.0.1",
    "downlevel-dts": "^0.6.0",
    "eslint": "^7.7.0",
    "fs-extra": "^9.0.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.2",
    "typescript-json-schema": "^0.43.0"
  },
  "types": "lib/index.d.ts",
  "typesVersions": {
    "<4.0": {
      "*": [
        "type-compat/ts3.4/index.d.ts"
      ]
    }
  }
}
