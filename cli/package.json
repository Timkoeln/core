{
  "name": "@gah/cli",
  "version": "0.7.0",
  "homepage": "https://github.com/gahjs/core",
  "repository": {
    "type": "git",
    "url": "https://github.com/gahjs/core.git"
  },
  "description": "Generic Angular Host Tooling",
  "main": "./lib/public-api.js",
  "files": [
    "lib",
    "assets"
  ],
  "bin": {
    "gah": "./lib/index.js"
  },
  "scripts": {
    "prebuild": "del-cli lib",
    "postbuild": "cpy src/templates lib/templates && cpy ../README.md ./",
    "start": "yarn run build && yarn run global",
    "global": "yarn global add file:D:/git/@gahjs/core/cli",
    "lint": "yarn eslint ./src/**",
    "build": "yarn run prebuild && tsc -p . && yarn run postbuild",
    "format": "prettier -w . --ignore-path .gitignore"
  },
  "engines": {
    "node": ">=12.0"
  },
  "author": "LoaderB0T",
  "license": "MIT",
  "private": false,
  "prettier": "@awdware/prettier-config",
  "dependencies": {
    "@gah/shared": "^0.7.0",
    "add": "^2.0.6",
    "awesome-logging": "~0.0.7",
    "awilix": "^4.3.1",
    "chalk": "^4.1.0",
    "change-case": "^4.1.2",
    "commander": "^7.0.0",
    "comment-json": "^4.1.0",
    "compare-versions": "^3.6.0",
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "deep-equal": "^2.0.5",
    "ejs": "^3.1.5",
    "enquirer": "^2.3.6",
    "figlet": "^1.5.0",
    "fs-extra": "^9.0.1",
    "fswin": "^3.21.107",
    "globby": "^11.0.2",
    "ora": "^5.2.0",
    "simple-git": "^2.31.0",
    "yarn": "^1.22.10"
  },
  "devDependencies": {
    "@awdware/eslint-config-nodejs": "^0.0.8",
    "@awdware/eslint-config-base": "^0.0.5",
    "@awdware/prettier-config": "^0.0.2",
    "@types/clear": "^0.1.1",
    "@types/decompress": "^4.2.3",
    "@types/deep-equal": "^1.0.1",
    "@types/ejs": "^3.0.5",
    "@types/figlet": "^1.2.1",
    "@types/fs-extra": "^9.0.6",
    "@types/node": "^14.14.20",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^4.13.0",
    "@typescript-eslint/parser": "^4.13.0",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.1",
    "eslint": "^7.17.0",
    "legally": "^3.5.9",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3"
