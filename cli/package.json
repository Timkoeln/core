    "compare-versions": "^3.6.0",
    "decompress": "^4.2.1",
    "decompress-targz": "^4.1.1",
    "deep-equal": "^2.0.4",
    "ejs": "^3.1.5",
    "enquirer": "^2.3.6",
    "figlet": "^1.5.0",
    "fs-extra": "^9.0.1",
