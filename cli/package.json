{
  "name": "@awdware/gah",
  "version": "0.0.77",
  "homepage": "https://github.com/awdware/gah",
  "repository": {
    "type": "git",
    "url": "https://github.com/awdware/gah.git"
  },
  "description": "Generic Angular Host Tooling",
  "main": "./lib/public-api.js",
  "files": [
    "lib",
    "assets"
  ],
  "bin": {
    "gah": "./lib/index.js"
  },
  "scripts": {
    "prebuild": "del-cli lib",
    "postbuild": "cpy src/templates lib/templates && cpy ../README.md ./",
    "start": "yarn run build && yarn run global",
    "global": "yarn global add file:F:/git/@awdware/gah/cli",
    "lint": "tslint -p . --fix",
    "build": "yarn run prebuild && tsc -p . && yarn run postbuild",
    "drop": "yarn run build && yarn publish --access public --no-git-tag-version "
  },
  "engines": {
    "node": ">=10.0"
  },
  "author": "LoaderB0T",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "@awdware/gah-shared": "^0.0.35",
    "chalk": "^4.1.0",
    "change-case": "^4.1.1",
    "commander": "^5.1.0",
    "comment-json": "^3.0.2",
    "ejs": "^3.1.2",
    "enquirer": "^2.3.6",
    "figlet": "^1.5.0",
    "fs-extra": "^9.0.1",
    "fswin": "^3.19.908",
    "globby": "^11.0.1",
    "inversify": "^5.0.1",
    "ora": "^4.0.4",
    "reflect-metadata": "~0.1.13"
  },
  "devDependencies": {
    "@types/clear": "^0.1.0",
    "@types/ejs": "^3.0.2",
    "@types/figlet": "^1.2.0",
    "@types/fs-extra": "^9.0.1",
    "@types/node": "^14.0.23",
    "@types/rimraf": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "cpy-cli": "^3.1.1",
    "del-cli": "^3.0.0",
    "eslint": "^7.4.0",
    "legally": "^3.5.6",
    "ts-node": "^8.10.1",
    "typescript": "~3.9.7"
  }
}
